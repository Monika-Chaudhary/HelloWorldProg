pipeline{
  agent any
  
  stages{
    stage('hello'){
      steps{
       echo 'Hello World'
      }
    }
    stage('Build'){
      steps{
        echo 'Building'
      }
    }
    stage('Deploy'){
      steps{
       echo 'Deploying'
      }
    }
    stage('Testing'){
      steps{
        echo 'Testing'
      }
    }
    stage('Release'){
      steps{
        echo 'Release'
      }
    }
  }
}
