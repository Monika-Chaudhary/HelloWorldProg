pipeline{
  agent any
  
  stages('hello'){
    step{
       echo 'Hello World'
    }
  }
  stages('Build'){
    step{
       echo 'Building'
    }
  }
  stages('Deploy'){
    step{
       echo 'Deploying'
    }
  }
  stages('Testing'){
    step{
      echo 'Testing'
    }
  }
  stages('Release'){
    step{
      echo 'Release'
    }
  }
}
